          "_customHTML": "<!-- Type valid HTML here -->\n\n\n<section data-bs-version=\"5.1\" class=\"form1 supplym5\" group=\"Forms\">\n    <!-- Remove or comment out this section -->\n<!--\n<mbr-parameters>\n    <header>Size</header>\n    <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n    <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n    <header>Background</header>\n    <fieldset type=\"background\" name=\"bg\" parallax>\n        <input type=\"color\" title=\"Color\" value=\"#e8e8e8\" selected>\n    </fieldset>\n</mbr-parameters>\n-->\n\n    <div class=\"mbr-fallback-image disabled\"></div>\n    <div>\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-6 mx-auto mbr-form\">\n                <form action=\"#\" name=\"ContactForm\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                    <div class=\"dragArea row\">\n                        <div class=\"col-12\">\n                            <h1 class=\"mbr-section-title mbr-fonts-style\">Send your message</h1>\n                        </div>\n                        \n                        <!-- Name Field -->\n                        <div data-for=\"name\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <input type=\"text\" name=\"name\" placeholder=\"Name\" data-form-field=\"Name\" class=\"form-control\" required=\"\">\n                        </div>\n                        \n                        <!-- Phone Number Field -->\n                        <div data-for=\"phone\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <input type=\"tel\" name=\"phone\" placeholder=\"Phone Number\" data-form-field=\"Phone\" class=\"form-control\" required=\"\">\n                        </div>\n\n                        <!-- Venue Name and Postal Code Field -->\n                        <div data-for=\"venue-name\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <input type=\"text\" name=\"venue-name\" placeholder=\"Venue Name and Postal Code\" data-form-field=\"Venue Name\" class=\"form-control\" required=\"\">\n                        </div>\n\n                        <!-- Event Date Field -->\n                        <div data-for=\"event-date\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <input type=\"date\" name=\"event-date\" placeholder=\"Event Date\" data-form-field=\"Event Date\" class=\"form-control\" required=\"\">\n                        </div>\n\n                        <!-- Event Time Field -->\n                        <div data-for=\"event-time\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <input type=\"time\" name=\"event-time\" placeholder=\"Event Time\" data-form-field=\"Event Time\" class=\"form-control\" required=\"\">\n                        </div>\n\n                        <!-- Installation Location Field -->\n                        <div data-for=\"installation-location\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <label class=\"form-control-label\">Where would you like to install the decorations?</label>\n                            <select name=\"installation-location\" class=\"form-control\" data-form-field=\"Installation Location\" required=\"\">\n                                <option value=\"\" disabled=\"\" selected=\"\">Select Location</option>\n                                <option value=\"Indoors\">Indoors</option>\n                                <option value=\"Outdoors\">Outdoors</option>\n                            </select>\n                        </div>\n\n                        <!-- Budget Field -->\n                        <div data-for=\"budget\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <label class=\"form-control-label\">Budget (in CAD)</label>\n                            <select name=\"budget\" class=\"form-control\" data-form-field=\"Budget\" required=\"\">\n                                <option value=\"\" disabled=\"\" selected=\"\">Select Budget</option>\n                                <option value=\"100-300\">$100 - $300</option>\n                                <option value=\"300-500\">$300 - $500</option>\n                                <option value=\"500-1000\">$500 - $1000</option>\n                                <option value=\"1000+\">Above $1000</option>\n                            </select>\n                        </div>\n\n                        <!-- Theme Field -->\n                        <div data-for=\"theme\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <input type=\"text\" name=\"theme\" placeholder=\"Colours/Theme\" data-form-field=\"Theme\" class=\"form-control\">\n                        </div>\n\n                        <!-- Occasion Description Field -->\n                        <div data-for=\"description\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <textarea name=\"description\" placeholder=\"Provide description of the occasion\" data-form-field=\"Description\" class=\"form-control\" rows=\"4\"></textarea>\n                        </div>\n\n                        <!-- Upload Pictures Field -->\n                        <div data-for=\"examples\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <label class=\"form-control-label\">Upload pictures of any examples that resemble the design you are looking for</label>\n                            <input type=\"file\" name=\"examples\" data-form-field=\"Examples\" class=\"form-control\" accept=\"image/*\" multiple=\"\">\n                        </div>\n\n                        <!-- Submit Button -->\n                        <div class=\"col-12 col-md-auto mbr-section-btn\">\n                            <button type=\"submit\" class=\"btn btn-primary\">SEND</button>\n                        </div>\n                    </div>\n                </form>\n            </div>\n        </div>\n    </div>\n</section>",
          "_customCSS": "mbr-form {\n    padding: 53px 60px;\n}\n\n.mbr-section-title {\n    color: #101511;\n    text-align: center;\n    font-size: 2rem;\n    margin-bottom: 1.5rem;\n}\n\n.form-control {\n    padding: 15px;\n    background-color: #ffffff;\n    border: 1px solid #cccccc;\n    border-radius: 10px;\n    font-size: 1rem;\n}\n\n.form-group label.form-control-label {\n    font-size: 1rem;\n    color: #333333;\n    margin-bottom: 8px;\n    display: block;\n}\n\n.form-group select.form-control,\n.form-group textarea.form-control {\n    padding: 10px;\n    background-color: #ffffff;\n    border: 1px solid #cccccc;\n    border-radius: 10px;\n}\n\n.form-group .btn-primary {\n    background-color: #007bff;\n    border: none;\n    padding: 12px 20px;\n    font-size: 1rem;\n    color: #ffffff;\n    border-radius: 10px;\n    width: 100%;\n}\n\n@media (max-width: 992px) {\n    .mbr-form {\n        padding: 20px;\n    }\n\n    .mbr-section-title {\n        font-size: 1.5rem;\n    }\n}\n",
